{
  "name": "react-skeleton",
  "version": "0.0.1",
  "description": "Boostraping react",
  "main": "app.html",
  "dependencies": {
    "react": "^0.12.2"
  },
  "devDependencies": {
    "browserify": "^8.1.0",
    "chai": "^1.9.1",
    "livereload": "^0.3.5",
    "mocha": "^1.21.4",
    "reactify": "^0.17.0",
    "sinon": "^1.10.3",
    "watchify": "^1.0.2"
  },
  "scripts": {
    "watch-js": "watchify -t reactify public/scripts/*.js -d -o build/app.js",
    "watch-reload": "livereload -p build/ & npm run watch-js",
    "build-js": "browserify -t reactify public/scripts/*.js > build/app.js",
    "build": "npm run build-css && npm run build-js",
    "test": "mocha -R spec -u bdd"
  },
  "author": "Xebia",
  "license": "MIT"
}
